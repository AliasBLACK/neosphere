<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-sphere">
    <magic priority="50">
      <match type="string" offset="0" value=".spk"/>
    </magic>
    <glob weight="100" pattern="*.spk"/>
    <glob weight="100" pattern="*.sgm"/>
    <glob weight="100" pattern="*.s2gm "/>
  </mime-type>
</mime-info>
